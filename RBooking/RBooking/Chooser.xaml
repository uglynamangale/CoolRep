<Window x:Class="RBooking.Chooser"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RBooking"
        mc:Ignorable="d"
        Title="MainWindow" Height="300" Width="600">
    <Grid>
        <Button x:Name="CheckBook" Content="Посмотреть бронь" HorizontalAlignment="Center" Margin="0,147,0,0" VerticalAlignment="Top" Height="46" Width="300" FontSize="20" Click="CheckBook_Click"/>
        <Button x:Name="Book" Content="Забронировать" HorizontalAlignment="Center" Margin="0,96,0,0" VerticalAlignment="Top" Height="46" Width="300" FontSize="20" Click="Book_Click"/>

    </Grid>
</Window>
